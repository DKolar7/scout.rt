<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.scout.rt.testing"
      label="%featureName"
      version="3.9.0.qualifier"
      provider-name="%providerName"
      plugin="org.eclipse.scout.rt.feature.about">

   <description url="http://www.eclipse.org/scout/">
      %description
