<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.scout.service.services">
      <service
            class="org.eclipse.scout.rt.ui.html.json.JsonMessageRequestInterceptor"
            createImmediately="false">
      </service>
      <service
            class="org.eclipse.scout.rt.ui.html.res.StaticResourceRequestInterceptor"
            createImmediately="false">
      </service>
      <service
            class="org.eclipse.scout.rt.ui.html.HtmlTextProviderService"
            createImmediately="false">
      </service>
      <service class="org.eclipse.scout.rt.ui.html.services.common.icon.HtmlBundleIconProviderService"
            createImmediately="false"
            factory="org.eclipse.scout.service.DefaultServiceFactory"
            ranking="-1000">
      </service>
      <service
            class="org.eclipse.scout.rt.ui.html.json.DefaultValuesFilterService"
            createImmediately="false">
      </service>
      <service
            class="org.eclipse.scout.rt.ui.html.res.WebContentService"
            createImmediately="false">
      </service>
   </extension>
</plugin>
