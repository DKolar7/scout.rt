<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="app"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.scout.svg.client.internal.TestApplication">
         </run>
      </application>
   </extension>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.scout.svg.client.app"
            name="SVG Client Test">
         <property
               name="appName"
               value="SVG Client Test">
         </property>
      </product>
   </extension>

</plugin>




